{
  "version": "0.1.0",
  "private": true,
  "name": "colorforcode",
  "authors": "Devin Jackson & Chloe Rice",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackson-/colorforcode.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router-dom",
    "express",
    "node",
    "postgresql",
    "boilerplate"
  ],
  "boilerplate-repos": {
    "create-react-app": "https://www. github.com/facebookincubator/create-react-app.git",
    "bones": "https://www. github.com/FullstackAcademy/bones.git",
    "react-redux-router-node": "https://github.com/chloerice/react-redux-router-node.git"
  },
  "proxy": "http://localhost:1337/",
  "scripts": {
    "start-dev": "concurrently \"npm run serve-dev\" \"node scripts/start.js\"",
    "build": "node scripts/build.js",
    "test-react": "node scripts/test.js --env=jsdom",
    "dev": "node dev",
    "validate": "check-node-version --node '>= 6.10.2'",
    "setup": "./bin/setup",
    "prep": "npm run validate && npm run setup",
    "postinstall": "npm run prep",
    "build-branch": "bin/build-branch.sh",
    "start": "node server/start.js",
    "serve-watch": "nodemon server/start.js --watch server --watch db --watch index.js --watch package.json",
    "serve-dev": "cross-env NODE_ENV=development npm run serve-watch",
    "test-node": "mocha --watch-extensions server/**/*.test.js db/**/*.test.js",
    "test-node-watch": "npm run test-node -- --watch --reporter=min",
    "seed": "node db/seed.js",
    "deploy-heroku": "bin/deploy-heroku.sh",
    "prepush": "npm run test-node",
    "upstream": "node c4c-ansible-deploy/ansible-deploy.js",
    "prereceive": "npm run upstream"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackson-/colorforcode.git/issues"
  },
  "homepage": "http://colorforcode.com",
  "dependencies": {
    "aws-sdk": "^2.73.0",
    "axios": "^0.15.2",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "check-node-version": "^1.1.2",
    "child-process": "^1.0.2",
    "cookie-session": "^2.0.0-alpha.1",
    "cors": "^2.8.3",
    "elasticsearch": "^13.0.1",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "fast-async": "^6.2.2",
    "finalhandler": "^1.0.0",
    "firebase": "^4.3.1",
    "homedir": "^0.6.0",
    "jquery": "^3.2.1",
    "node-ansible": "^0.5.5",
    "nodemailer": "^4.0.1",
    "passport": "^0.3.2",
    "passport-linkedin": "^1.0.0",
    "passport-local": "^1.0.0",
    "payment": "^1.0.4",
    "pg": "^6.1.0",
    "pretty-error": "^2.0.2",
    "prop-types": "^15.5.10",
    "raven": "^2.3.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-typeahead": "^2.0.0-alpha.3",
    "react-dom": "^15.3.2",
    "react-fa": "^4.2.0",
    "react-redux": "^4.4.5",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.1",
    "react-router-hash-link": "^1.1.0",
    "react-rte": "^0.15.0",
    "react-select": "^1.0.0-rc.5",
    "react-virtualized-select": "^3.0.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.0",
    "sendmail": "^1.1.1",
    "sequelize": "^3.24.6",
    "stripe": "^4.19.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-preset-react-app": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.4.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "^4.0.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "husky": "^0.13.3",
    "jest": "17.0.2",
    "json-loader": "0.5.4",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "2.1.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "supertest": "^3.0.0",
    "url-loader": "0.5.7",
    "volleyball": "^1.4.1",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
